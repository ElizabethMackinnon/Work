# Alphabetize
roi_fa = [
    'PTR, EC, CGC, SFO, IFO, UNC, FX, BCC, GCC', 
    'PTR, SS, CGC, FX.ST, SLF, SFO, IFO, BCC, GCC, SCC',
    'ALIC, PLIC, RLIC, ACR, SCR, PCR, PTR, SS, EC, CGC, FX.ST, SLF, SFO, UNC, IC, FX, BCC, GCC, SCC',
    'CST, ALIC, PLIC, RLIC, ACR, PCR, EC, CGC, CGH, SLF, SFO, IFO, IC, BCC, GCC, SCC',
    'SS, CGC, SLF',
    'PLIC, RLIC, ACR, SCR, PCR, PTR, SS, EC, CGC, FX.ST, SLF, SFO, UNC, IC, FX, BCC',
    'CST, ALIC, PLIC, RLIC, PTR, EC, CGC, CGH, SLF, SFO, IC',
    '',
    '',
    '',
    'CST, ALIC, ACR, PCR, SS, EC, CGC, SLF, SFO, UNC',
    '',
    '',
    '',
]

def alphabetize_string(s):
    if s: 
        items = s.split(', ')
        items.sort()
        return ', '.join(items)
    return s

roi_fa_alphabetized = [alphabetize_string(s) for s in roi_fa]

print("Alphabetized list:")
for s in roi_fa_alphabetized:
    if s: 
        print(s)
    else:
        print("[Empty String]")  

print("Finished printing the alphabetized list.")

roi_md = [
